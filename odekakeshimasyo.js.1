$((function(){
	function logo() {
		if (color = $.cookie('site-color')) $('header').attr('class', color);
		$logo = $('header h2');
		$logo.html($logo.html().replace(/.io/g, '<span class="site-color-orange">.</span><span class="site-color-blue">i</span><span class="site-color-pink">o</span>'));
		$('.site-color-pink, .site-color-blue, .site-color-orange').click(function(){
			var color = $(this).attr('class').split(' ')[0];
			$('header').attr('class', color);
			$.cookie('site-color', color);
		});
	}
	return function() {
		logo();
	}
})());
